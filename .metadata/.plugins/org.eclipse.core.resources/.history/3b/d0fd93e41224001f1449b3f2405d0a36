public class DisplayPattern {
    public static void main(String[] args) {
        int n = 10;

        for (int i = n; i >= 5; i -= 2) {
            for (int j = 0; j < i; j++) {
                System.out.print("*");
            }
            System.out.println();
        }

        for (int i = 5; i <= n; i += 2) {
            for (int j = 0; j < i; j++) {
                System.out.print("*");
            }
            System.out.println();
        }
    }
}
